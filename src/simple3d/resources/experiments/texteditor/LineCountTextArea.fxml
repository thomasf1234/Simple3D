<?xml version="1.0" encoding="UTF-8"?>



<?import experiments.texteditor.UnfocusableTextArea?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="experiments.texteditor.LineCountTextArea"
      prefHeight="400.0" prefWidth="600.0">
    <UnfocusableTextArea id="lineCountTextArea"
                         wrapText="false"
                         disable="true"
                         editable="false"
                         focusTraversable="false"
                         maxWidth="100"
                         nodeOrientation="RIGHT_TO_LEFT">
        <font>
            <Font name="Courier" size="14">
            </Font>
        </font>
    </UnfocusableTextArea>
    <LineCountTextArea >
        <font>
            <Font name="Courier" size="14">
            </Font>
        </font>
    </LineCountTextArea>
</VBox>
