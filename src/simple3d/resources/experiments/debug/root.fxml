<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="experiments.debug.controllers.RootController"
            fx:id="root"
            prefHeight="400.0" prefWidth="600.0">
    <center>
        <SplitPane dividerPositions="0.3" orientation="HORIZONTAL">
            <items>
                <TreeView fx:id="nodeTreeView" maxWidth="Infinity">

                </TreeView>
                <TextArea fx:id="debugTextArea" editable="false">
                    <font>
                        <Font name="Courier" size="14">
                        </Font>
                    </font>
                </TextArea>
            </items>
        </SplitPane>
    </center>
</BorderPane>
